%h1 Listing settings

%table
  %tr
    %th Value
    %th Setup group
    %th Tuning
    %th Setting type
    %th
    %th
    %th

  - @settings.each do |setting|
    %tr
      %td= setting.value
      %td= setting.setup_group_id
      %td= setting.tuning_id
      %td= setting.setting_type_id
      %td= link_to 'Show', setting
      %td= link_to 'Edit', edit_setting_path(setting)
      %td= link_to 'Destroy', setting, :confirm => 'Are you sure?', :method => :delete

%br

= link_to 'New Setting', new_setting_path
